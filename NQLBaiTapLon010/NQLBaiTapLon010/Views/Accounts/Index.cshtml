@model IEnumerable<NQLBaiTapLon010.Models.Account>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Tạo mới", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PassWord)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RoleID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PassWord)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RoleID)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id=item.UseName }) |
            @Html.ActionLink("Chi tiết", "Details", new { id=item.UseName }) |
            @Html.ActionLink("Xóa", "Delete", new { id=item.UseName })
        </td>
    </tr>
}

</table>
